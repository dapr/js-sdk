{
  "name": "@roadwork/dapr-js-sdk-example-hello-world-distributed",
  "version": "1.0.0",
  "description": "An example utilizing the Dapr JS SDK",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/index.js",
    "start:dev": "npm run build && nodemon --ext \".ts,.js\" --watch \"./src\" --exec \"ts-node ./src/index.ts\"",
    "start:dapr": "dapr run --app-id my-server --dapr-http-port 3500 --app-port 4000 npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@roadwork/dapr-js-sdk": "file:../../../../build",
    "@types/node": "^15.3.0"
  }
}
